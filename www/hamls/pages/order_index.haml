%header
  订餐
  .header-left
    %a.btn-style.btn-4(ng-tap = 'back_to_home_page()')
      Back
#wrapper.wrapper.scrollable
  %form.wrapper-inner
    %fieldset.form-wrapper
      .break20
        %h5 人:
        %input.input-sm.input-full.input-large.form-control(type="text" ng-disabled='true')
        %a.btn-style.btn-3.text-center(style="margin-top:10px" ng-tap = 'go_to_select_people_page()')
          选人
      .break20
        %h5 餐厅:
        %input.input-sm.input-full.input-large.form-control(type="text" ng-disabled='true')
        %a.btn-style.btn-3.text-center(style="margin-top:10px" ng-tap = 'go_to_select_restaurant_page()')
          选餐厅
      .break20
        %h5 套餐:
        %input.input-sm.input-full.input-large.form-control(type="text" ng-disabled='true')
        %a.btn-style.btn-3.text-center(style="margin-top:10px" ng-tap = 'go_to_select_set_page()')
          选套餐
      .break20
        %a.btn-style.btn-3.text-center(ng-tap = 'submit_ordering()')
          确认
